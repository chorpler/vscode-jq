{
  "name": "jq-preview",
  "displayName": "jq Live View",
  "description": "A sample JQ for virtual document",
  "icon": "icon.png",
  "version": "0.0.1",
  "publisher": "ldd-vs-code",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldd/vscode-jq"
  },
  "bugs": {
    "url": "https://github.com/ldd/vscode-jq/issues"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jq.showPreview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "jq Live View",
      "properties": {
        "jq.customCommand": {
          "type": "string",
          "default": "jq '$$user_filter' $$file_path",
          "markdownDescription": "Custom command that supports jq command line options.\n\n `$$user_filter` is replaced with the filter you typed\n\n `$$file_path` is replaced with the current file path"
        }
      }
    },
    "commands": [
      {
        "command": "jq.showPreview",
        "title": "jq"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "jq.showPreview",
          "group": "navigation",
          "when": "resourceLangId == json && resourceScheme != jq"
        }
      ],
      "commandPalette": [
        {
          "command": "jq.showPreview",
          "when": "resourceLangId == json"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "prettier": "^1.19.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  }
}
